<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.drangies.coupDeFoudre" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Hello Cordova</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <access origin="*" />
    <preference name="loglevel" value="DEBUG" />
    <feature name="App">
        <param name="android-package" value="org.apache.cordova.App" />
    </feature>
    <feature name="Serial">
        <param name="android-package" value="org.stereolux.cordova.serial.Serial" />
    </feature>
    <author email="xavier.seignard@gmail.com" href="http://drangies.fr">
        Xavier Seignard
    </author>
    <content src="index.html" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <icon cdv:density="ldpi" cdv:platform="android" src="res/icon/android/icon-36-ldpi.png" />
    <icon cdv:density="mdpi" cdv:platform="android" src="res/icon/android/icon-48-mdpi.png" />
    <icon cdv:density="hdpi" cdv:platform="android" src="res/icon/android/icon-72-hdpi.png" />
    <icon cdv:density="xhdpi" cdv:platform="android" src="res/icon/android/icon-96-xhdpi.png" />
</widget>
