    <icon src="res/icon/android/icon-48-mdpi.png" cdv:platform="android" cdv:density="mdpi" />
    <icon src="res/icon/android/icon-72-hdpi.png" cdv:platform="android" cdv:density="hdpi" />
    <icon src="res/icon/android/icon-96-xhdpi.png" cdv:platform="android" cdv:density="xhdpi" />
</widget>
